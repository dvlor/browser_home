{
  "name": "browser_home",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-browser_home",
    "stop": "egg-scripts stop --title=egg-server-browser_home",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "build": "pkg . --targets node14-linux-64 --out-path ./dist/ --debug"
  },
  "dependencies": {
    "@types/egg": "^1.5.0",
    "egg": "^2.6.1",
    "egg-scripts": "^2.15.2",
    "egg-view-nunjucks": "^2.3.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "mocha": "5.2.0",
    "tslib": "^2.3.1",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "",
  "license": "MIT",
  "pkg": {
    "scripts": [
      "./app/**/*.ts",
      "./config/**/*"
    ],
    "assets": [
      "./app/public/**/*",
      "./node_modules/**/*"
    ]
  }
}
